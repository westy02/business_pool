<h1>Listing subskillsets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subskillsets.each do |subskillset| %>
      <tr>
        <td><%= subskillset.name %></td>
        <td><%= link_to 'Show', subskillset %></td>
        <td><%= link_to 'Edit', edit_subskillset_path(subskillset) %></td>
        <td><%= link_to 'Destroy', subskillset, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subskillset', new_subskillset_path %>
